{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "isbn",
    "amazon_url",
    "author",
    "language",
    "pages",
    "publisher",
    "title",
    "year"
  ],
  "properties": {
    "isbn": {
      "type": "string",
      "default": "",
      "title": "The isbn Schema",
      "examples": ["151515 "]
    },
    "amazon_url": {
      "type": "string",
      "default": "",
      "title": "The amazon_url Schema",
      "examples": ["fakeUrl"]
    },
    "author": {
      "type": "string",
      "default": "",
      "title": "The author Schema",
      "examples": ["fakeAuthor"]
    },
    "language": {
      "type": "string",
      "default": "",
      "title": "The language Schema",
      "examples": ["english"]
    },
    "pages": {
      "type": "integer",
      "default": 0,
      "title": "The pages Schema",
      "examples": [123]
    },
    "publisher": {
      "type": "string",
      "default": "",
      "title": "The publisher Schema",
      "examples": ["fakePub"]
    },
    "title": {
      "type": "string",
      "default": "",
      "title": "The title Schema",
      "examples": ["faketitle"]
    },
    "year": {
      "type": "integer",
      "default": 0,
      "minimum": 0,
      "maximum": 2030,
      "title": "The year Schema",
      "examples": [2022]
    }
  },
  "additionalProperties": false,
  "examples": [
    {
      "isbn": "151515 ",
      "amazon_url": "fakeUrl",
      "author": "fakeAuthor",
      "language": "english",
      "pages": "123-123-1234",
      "publisher": "fakePub",
      "title": "faketitle",
      "year": 2022
    }
  ]
}
